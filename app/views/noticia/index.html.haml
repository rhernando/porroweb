%h1 Listing noticia

%table
  %tr
    %th Texto
    %th Fecha
    %th Titulo
    %th
    %th
    %th

  - @noticia.each do |noticium|
    %tr
      %td= noticium.texto
      %td= noticium.fecha
      %td= noticium.titulo
      %td= link_to 'Show', noticium
      %td= link_to 'Edit', edit_noticium_path(noticium)
      %td= link_to 'Destroy', noticium, :confirm => 'Are you sure?', :method => :delete

%br

= link_to 'New Noticium', new_noticium_path
